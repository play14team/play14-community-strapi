{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string"
    },
    "date": {
      "type": "string"
    },
    "start": {
      "type": "datetime"
    },
    "end": {
      "type": "datetime"
    },
    "description": {
      "type": "richtext"
    },
    "venue": {
      "model": "location",
      "via": "events"
    },
    "location": {
      "model": "event-location",
      "via": "events"
    },
    "sponsors": {
      "collection": "sponsor",
      "via": "events",
      "dominant": true
    },
    "Pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
